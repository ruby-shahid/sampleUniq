<style>
tr.hidden {
    display: none;
}
</style>
<p id="notice"><%= notice %></p>
<table class="table table-striped table-narrow">
  <thead>
    <tr>
      <th>Column <%= @document.compare_col%> Comparision</th>
    </tr>
  </thead>
  <tbody>
    <% @document.get_dup_attributes.each do |k,v| %>
      <% k.times do %>
        <% v.uniq.each do |vv| %>
        <tr class="hidden">
          <td><%= vv %></td>
          <% end %>
        </tr>
        <% end %>
    <% end %>
  </tbody>    

<%= link_to 'Edit', edit_document_path(@document) %> |
<%= link_to 'Back', documents_path %>
<script type="text/javascript">
  $(document).ready(function(){
    if ($(".table tbody tr").length > 1){
      $(".table tbody tr").each(function(i,el){
        setTimeout(function (){
          $(".table tbody tr").eq(i).show();
        }, 3000 + (i * 3000));
      })
    }
  });
</script>
